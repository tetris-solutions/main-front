{
  "name": "@tetris/main-front",
  "scope": "tetris",
  "version": "0.0.1",
  "description": "front para o site e admin base",
  "main": "lib/main.js",
  "engineStrict": true,
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=4.0.0"
  },
  "bin": {
    "main-front": "bin/cmd.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build:commonjs": "rimraf lib && babel src -d lib",
    "build:documentation": "scripts/gen-docs.js",
    "build:client": "scripts/bundle.js",
    "test": "scripts/test.js",
    "babel-node": "babel-node",
    "prepublish": "npm run build:commonjs",
    "lint": "eslint src",
    "start": "bin/cmd.js",
    "start:dev": "nodemon src/main.js"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetris-solutions/main-front.git"
  },
  "bugs": {
    "url": "https://github.com/tetris-solutions/main-front/issues"
  },
  "homepage": "https://github.com/tetris-solutions/main-front#readme",
  "dependencies": {
    "@tetris/base-lib": "^0.1.0",
    "@tetris/http": "^0.2.3",
    "baobab": "^2.3.3",
    "baobab-react": "^2.1.1",
    "classnames": "^2.2.3",
    "csjs": "^1.0.3",
    "dotenv": "^2.0.0",
    "global": "^4.3.0",
    "js-cookie": "^2.1.0",
    "lodash": "^4.11.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-intl": "^1.2.2",
    "react-router": "^2.7.0",
    "tetris-iso": "^0.2.0",
    "winston": "^2.2.0",
    "winston-logstash": "^0.2.11"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "cheerio": "^0.20.0",
    "delay": "^1.3.1",
    "documentation": "^4.0.0-beta",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "jsdom": "^8.1.0",
    "mock-require": "^1.2.1",
    "node-mocks-http": "^1.5.2",
    "nodemon": "^1.9.1",
    "pre-commit": "^1.1.2",
    "react-addons-test-utils": "^15.2.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "shelljs": "^0.6.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "ava": {
    "files": [
      "lib/**/*.spec.js"
    ],
    "failFast": true,
    "verbose": true,
    "tap": false
  }
}
